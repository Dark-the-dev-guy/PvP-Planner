    "discord.js": "^14.16.3",
    "dotenv": "^16.3.1",
    "mongoose": "^7.5.0",
    "uuid": "^9.0.0",
    "vite": "^6.0.5"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^7.5.0",
    "eslint": "^8.50.0",
    "postcss": "^8.4.49",
    "prettier": "^3.0.0"
  },
  "resolutions": {
    "serialize-javascript": "^5.0.1",
    "lodash.template": "^4.5.0",
    "loader-utils": "^2.0.0",
    "http-proxy-middleware": "^3.0.0",
    "cross-spawn": "^7.0.0"
  },
