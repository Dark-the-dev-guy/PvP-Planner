{
  "name": "PvP-Planner",
  "version": "1.0.0",
  "description": "PvP Planner with Discord bot and frontend interface.",
  "main": "backend/index.js",
  "scripts": {
    "build": "npm install --prefix frontend && npm run build --prefix frontend",
    "start": "node backend/index.js",
    "deploy-commands": "node backend/deploy-commands.js",
    "migrate": "node backend/scripts/migrateUserTagsToIDs.js",
